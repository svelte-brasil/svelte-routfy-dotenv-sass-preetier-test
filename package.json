{
  "name": "svelte-app",
  "version": "1.0.0",
  "scripts": {
    "dev": "run-p routify rollup",
    "dev-dynamic": "cross-env BUNDLING=dynamic npm run dev",
    "build": "npm run routify -- -b && rollup -c",
    "serve": "spassr --serve-spa --serve-ssr",
    "export": "routify export",
    "build:docker": "npm run build && ./scripts/docker/docker-build.sh",
    "deploy:now": "cd scripts/now && npm run deploy",
    "deploy:netlify": "cd scripts/netlify && npm run deploy",
    "rollup": "rollup -cw",
    "routify": "routify -D",
    "test": "jest src --watchAll"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.6",
    "@babel/plugin-transform-strict-mode": "^7.8.3",
    "@babel/preset-env": "^7.9.6",
    "@babel/register": "^7.9.0",
    "@rollup/plugin-alias": "^3.1.0",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@testing-library/jest-dom": "^5.5.0",
    "@testing-library/svelte": "^3.0.0",
    "autoprefixer": "^9.8.0",
    "babel-jest": "^25.5.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-add-module-exports": "^1.0.2",
    "babel-plugin-inline-dotenv": "^1.5.0",
    "concurrently": "^5.2.0",
    "cross-env": "^7.0.2",
    "del": "^5.1.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-svelte3": "^2.7.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.5.4",
    "jest-dom": "^4.0.0",
    "jest-transform-svelte": "^2.1.1",
    "node-sass": "^7.0.3",
    "npm-run-all": "^4.1.5",
    "ppid-changed": "^1.0.1",
    "rollup": "^2.10.2",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-livereload": "^1.3.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-svelte": "^5.2.2",
    "rollup-plugin-terser": "^5.3.0",
    "sass-loader": "^8.0.2",
    "spassr": "^1.0.2",
    "style-loader": "^1.2.1",
    "svelte": "^3.49.0",
    "svelte-loader": "^2.13.6",
    "svelte-preprocess": "^3.7.4"
  },
  "dependencies": {
    "@sveltech/routify": "^1.5.9"
  }
}
